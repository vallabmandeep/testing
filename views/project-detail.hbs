


 

  <div class="album py-5 bg-light">
    <div class="container">

      <div class="row">
        <div class="col-md-3 project-sb">
          <div>
            <h2>Links</h2>
          </div>

          <div class="my-2">
            <a href="#" class="btn btn-primary">Live Demo</a>
          </div>

          <div class="my-2">
            <a href="{{project.githubUrl}}" class="btn btn-secondary">Github Link</a>
          </div>

          <hr>
          <div>
            <h2>Tags</h2>
            {{#each project.Tags}}

             <a href="#" class="badge badge-{{this.class}}">{{this.name}}</a>
            {{/each}}
          </div>


          <hr>
        
          <div>
            <h3>Related Projects</h3>
            {{#each project.relatedProjects}}
          </div>  

          <div  class="my-2">
            <a href="/projects/{{this.link}}">{{this.name}}</a>
          </div>
          {{/each}}
          </div>
      
        <div class="col-md-9">
          <nav aria-label="breadcrumb">
             <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">Home</a></li>
                <li class="breadcrumb-item"><a href="/projects">Projects</a></li>
                <li class="breadcrumb-item active" aria-current="page">{{project.name}}</li>
              </ol>
          </nav>
          <h1>{{project.name}}</h1>
          <p>{{project.description}}</p>
        </div>
        
      </div>
    </div>
  </div>

